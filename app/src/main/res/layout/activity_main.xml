                    android:layout_height="25dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="60dp"
                    android:text="k :"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:gravity="end"
