                    android:background="@color/colorWhite"/>

                <EditText
                    android:id="@+id/editText4"
                    android:layout_width="220dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="60dp"
                    android:layout_marginEnd="20dp"
